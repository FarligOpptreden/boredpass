<configuration>
  <appSettings>
    <add key="NODE_ENV" value="production" />
  </appSettings>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="app.iis.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <clear />
        <rule name="iisnode">
          <match url="/*" />
          <action type="Rewrite" url="app.iis.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>